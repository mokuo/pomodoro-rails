{
  "extends": "airbnb",
    "rules": {
      "semi": 0,
      "arrow-parens": 0,
      "comma-dangle": 0,
      "no-restricted-globals": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "globals": {
      "window": true,
      "location": true,
      "document": true
    },
    "env": {
      "jquery": true
    }
}
